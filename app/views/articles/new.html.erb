<%= form_for [@category, @article] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :desciption %>
  <%= f.text_area :description, size: "40x10" %><br>
  <%= f.label :price %>
  <%= f.number_field :price %><br>
  <%= f.label :contact %>
  <%= f.text_field :contact %><br>

  <%= f.submit %>
<% end %>

